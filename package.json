{
  "name": "react-project-template",
  "version": "0.0.0",
  "description": "a tempalate for quick start with React.js",
  "scripts": {
    "lint": "npm run lint:css --silent && npm run lint:js --silent",
    "lint:js": "eslint --ext .jsx --ext .js app server",
    "lint:css": "stylelint app/**/*.css",
    "fix": "eslint --fix --ext .jsx --ext .js app server",
    "pretest": "npm run lint --silent",
    "test": "karma start --single-run=true",
    "build": "NODE_ENV=production babel-node utils/build.js",
    "start": "NODE_ENV=development nodemon --watch server/* --exec babel-node server/server",
    "playground": "NODE_ENV=playground babel-node server/server",
    "production": "npm run build && NODE_ENV=production babel-node server/server",
    "commit": "commit-wizard"
  },
  "devDependencies": {
    "autoprefixer": "^6.2.3",
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.26",
    "babel-eslint": "^5.0.0-beta6",
    "babel-loader": "^6.2.0",
    "babel-plugin-flow-comments": "^6.3.19",
    "babel-plugin-react-display-name": "^2.0.0",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-source-wrapper": "^1.3.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-typecheck": "^3.5.1",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015-loose": "^6.1.4",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-1": "^6.3.13",
    "chai": "^3.4.1",
    "chai-things": "^0.2.0",
    "cosmos-js": "Nitive/cosmos",
    "css-loader": "^0.23.1",
    "debug": "^2.2.0",
    "enzyme": "^1.2.0",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^2.1.1",
    "eslint-plugin-react": "^3.13.1",
    "express": "^4.13.3",
    "express-debuga": "^1.0.0",
    "express-open-in-editor": "^1.1.0",
    "extract-text-webpack-plugin": "^0.9.1",
    "filesize": "^3.1.5",
    "gemini": "^2.0.3",
    "gemini-gui": "^2.0.0",
    "history": "^1.17.0",
    "karma": "^0.13.16",
    "karma-chai": "^0.1.0",
    "karma-chai-things": "^0.1.4",
    "karma-chrome-launcher": "^0.2.2",
    "karma-mocha": "^0.2.1",
    "karma-mocha-reporter": "^1.1.4",
    "karma-phantomjs-launcher": "^0.2.2",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.3.4",
    "morgan": "^1.6.1",
    "nyan-progress-webpack-plugin": "^1.1.4",
    "phantomjs": "^1.9.19",
    "phantomjs-polyfill": "0.0.1",
    "portscanner": "^1.0.0",
    "postcss": "^5.0.13",
    "postcss-autoreset": "^1.1.1",
    "postcss-loader": "^0.8.0",
    "postcss-normalize": "^0.1.2",
    "postcss-reporter": "^1.3.0",
    "pre-git": "^3.1.2",
    "precss": "^1.3.0",
    "progress-bar-webpack-plugin": "^1.2.0",
    "react": "^0.14.5",
    "react-addons-test-utils": "^0.14.5",
    "react-css-modules": "^3.6.3",
    "react-dom": "^0.14.5",
    "react-redux": "^4.0.6",
    "react-router": "^1.0.3",
    "react-transform-catch-errors": "^1.0.1",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.2.0",
    "redux": "^3.0.5",
    "redux-devtools": "^3.0.1",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.1",
    "redux-simple-router": "^1.0.2",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.0",
    "stylelint": "^3.1.4",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0",
    "yargs": "^3.31.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nitive/react-project-template.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "project",
    "template",
    "redux",
    "webpack",
    "babel",
    "eslint",
    "testing",
    "karma",
    "mocha",
    "chai",
    "enzyme",
    "component-inspector",
    "cosmos",
    "react-transform",
    "hot",
    "reload"
  ],
  "author": "Maxim Samoilov @nitive",
