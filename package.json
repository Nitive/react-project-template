  },
  "homepage": "https://github.com/Nitive/react-project-template#readme",
  "dependencies": {
    "colors": "^1.1.2",
    "jade": "^1.11.0",
    "node-bin": "^5.3.0"
  },
  "config": {
    "pre-git": {
      "commit-msg": "conventional",
      "pre-commit": [],
      "pre-push": [
        "npm test"
      ],
      "post-commit": [],
      "post-merge": []
    }
  }
}
