{
  "name": "react-project-template",
  "version": "0.0.0",
  "description": "a tempalate for quick start with React.js",
  "scripts": {
    "lint": "eslint --ext .jsx --ext .js app",
    "pretest": "npm run lint",
    "test": "karma start --single-run=true",
    "build": "NODE_ENV=production babel-node build.js",
    "dev": "DEBUG=react-project-template NODE_ENV=development nodemon --watch server/* --exec babel-node server/server",
    "start": "NODE_ENV=production babel-node server/server"
  },
  "dependencies": {
    "babel": "^5.8.29",
    "babel-core": "^5.8.29",
    "babel-loader": "^5.3.2",
    "babel-plugin-react-autoprefix": "^0.2.6",
    "express": "^4.13.3",
    "filesize": "^3.1.3",
    "history": "^1.13.0",
    "morgan": "^1.6.1",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-redux": "^4.0.0",
    "react-router": "^1.0.0-rc4",
    "redux": "^3.0.4",
    "redux-router": "^1.0.0-beta3",
    "webpack": "^1.12.2"
  },
  "devDependencies": {
    "babel-eslint": "^4.1.3",
    "babel-plugin-react-transform": "^1.1.1",
    "component-inspector": "^1.2.0",
    "debug": "^2.2.0",
    "eslint": "^1.7.3",
    "eslint-config-airbnb": "^0.1.0",
    "eslint-plugin-react": "^3.6.3",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.1.1",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.4.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nitive/react-project-template.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "project",
    "template",
    "redux",
    "webpack",
    "babel",
    "eslint",
    "karma",
    "react-transform",
    "hot",
    "reload"
  ],
  "author": "Maxim Samoilov @nitive",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Nitive/react-project-template/issues"
  },
  "homepage": "https://github.com/Nitive/react-project-template#readme"
}
